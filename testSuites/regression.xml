<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite" parallel="tests">

    <listeners>
        <listener class-name="TestComponents.Listeners"/>
    </listeners>

    <test name="Registration Test" thread-count="5">
        <classes>
            <class name="testCases.Register.RegistrationTestViaRegisterLink"/>
        </classes>
    </test> <!-- Test -->

    <test name="RegistrationTest Using Faker" thread-count="5">
        <classes>
            <class name="testCases.Register.RegistrationTest_Using_Faker_Input"/>
        </classes>
    </test> <!-- Test -->

    <test name="RegistrationTest Using Json Input" thread-count="5">
        <classes>
            <class name="testCases.Register.RegistrationTest_Using_Json_Input"/>
        </classes>
    </test> <!-- Test -->

</suite> <!-- Suite -->
